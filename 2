interface Meowable {
    void meow();
}
class Cat implements Meowable {
    private String name;
    private int meowCount = 0;
    public Cat(String name) {
        this.name = name;
    }
    public void meow() {
        meowCount++;
        System.out.println(name + ": мяу!");
    }
    public int getMeowCount() {
        return meowCount;
    }
}
class Funs {
    public static void meowsCare(Meowable m) {
        for (int i = 0; i < 5; i++) {
            m.meow();
        }
    }
}
public class Main {
    public static void main(String[] args) {
        Meowable m = new Cat("Кек");
        Funs.meowsCare(m);
        if (m instanceof Cat) {
            System.out.println("Кот мяукнул " + ((Cat) m).getMeowCount() + " раз");
        }
    }
}
